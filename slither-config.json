{
    "compile_force_framework": "Truffle",
    "detectors_to_exclude": "naming-convention|solc-version",
    "exclude_informational": false,
    "exclude_low": false,
    "exclude_medium": false,
    "exclude_high": false,
    "disable_color": false,
    "filter_paths": "@openzeppelin|interfaces|mock",
    "ignore_compile": true,
    "legacy_ast": false,
    "solc": "0.8.11",
    "truffle_build_directory": "./build/contracts",
    "truffle_ignore_compile": true
}
